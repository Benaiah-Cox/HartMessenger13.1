<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_Home_screen"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="@android:dimen/dialog_min_width_major"
    android:background="@color/colorAccent"
    tools:ignore="UnusedAttribute">
s    <android.support.v7.widget.Toolbar
        android:layout_height="40dp"
        android:layout_width="220dp"
        android:background="?attr/colorPrimary"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/toolbar1"
        android:elevation="10dp"
        android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@android:drawable/ic_menu_search"
        android:id="@+id/search_friends"
        android:background="@color/colorPrimary"
        android:elevation="10dp"
        android:adjustViewBounds="false"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/toolbar1"
        android:layout_toEndOf="@+id/toolbar1"
        tools:ignore="ContentDescription"/>
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@android:drawable/sym_action_chat"
        android:id="@+id/chat_menu"
        android:background="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/search_friends"
        android:layout_toEndOf="@+id/search_friends"
        tools:ignore="ContentDescription"
        android:elevation="10dp"
        />
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_perm_identity_white_48dp"
        android:id="@+id/contact_List"
        android:background="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/chat_menu"
        android:layout_toEndOf="@+id/chat_menu"
        tools:ignore="ContentDescription" android:elevation="10dp"/>
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/settings_Menu"
        tools:background="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/contact_List"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        tools:aaaignore="ContentDescription,RtlHardcoded"
        android:elevation="10dp"/>
</RelativeLayout>
